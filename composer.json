{
    "name": "fox/laravel-application-version",
    "description": "Fox's smarter application version manager",
    "keywords": ["artisan", "laravel", "application", "version"],
    "license": "MIT",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^7.2.1",
        "illuminate/contracts": "^5.7",
        "illuminate/support": "^5.7",
        "composer/composer": "^1.8",
        "symfony/process": "^4.2"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.8.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^8.0",
        "dmx/phpunit-ext": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Fox\\Application\\Version\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fox\\Application\\Version\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "style-fix": "vendor/bin/php-cs-fixer fix",
        "style-check": "vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fox\\Application\\Version\\ServiceProvider"
            ],
            "aliases": {
                "version-manager": "Fox\\Application\\Version\\VersionManager"
            }
        }
    }
}
