{
    "name": "fox/laravel-application-version",
    "description": "Fox's smarter application version manager",
    "keywords": [
        "artisan",
        "laravel",
        "application",
        "version"
    ],
    "license": "MIT",
    "type": "library",
    "version": "2.0.1",
    "require": {
        "php": ">=7.4.1",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "composer/composer": "^1.10",
        "symfony/process": "^4.0|^5.0"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5",
        "dmx/phpunit-ext": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Fox\\Application\\Version\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fox\\Application\\Version\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "style-fix": "vendor/bin/php-cs-fixer fix",
        "style-check": "vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fox\\Application\\Version\\ServiceProvider"
            ],
            "aliases": {
                "version-manager": "Fox\\Application\\Version\\VersionManager"
            }
        }
    }
}
